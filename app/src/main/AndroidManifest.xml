<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.demo.app">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.SYSTEM_OVERLAY_WINDOW" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning,UnusedAttribute">
        <activity android:name=".activity.LabelActivity" android:label="@string/event_label"/>
        <activity
            android:name=".activity.HighLightActivity"
            android:label="@string/high_light" />
        <activity
            android:name=".activity.StatusBarActivity"
            android:label="@string/status_bar_style" />
        <activity
            android:name=".activity.ToolbarActivity"
            android:label="@string/tool_bar_style" />
        <activity
            android:name=".activity.EventActivity"
            android:label="@string/event_transmit" />
        <activity
            android:name=".activity.FloatButtonActivity"
            android:label="@string/float_button" />
        <activity
            android:name=".activity.EditViewActivity"
            android:label="@string/edit_view" />
        <activity
            android:name=".activity.WebViewActivity"
            android:label="@string/fragment_web" />
        <activity
            android:name=".activity.VerificationCodeActivity"
            android:label="@string/verification_code" />
        <activity
            android:name=".activity.MainActivity"
            android:label="Demo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>